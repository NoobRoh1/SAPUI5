<mvc:View controllerName="com.demo.sapui5.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
      <content>
        <!-- <Button text="Register Employee Details" press="Press"></Button> -->
        <Text id="main" text = "Employee Details Page" textAlign="Center" width="100%" class = "fontsize"></Text>
        <Label id="fname" text="FirstName" required="true" class="sapUiTinyMarginBegin"></Label>
        <Input id="Inputf" width="30%" maxLength="15" class="sapUiTinyMarginBegin" />
        <Label id="lname" text="LastName:" required="false" class="sapUiTinyMarginBegin"></Label>
        <Input id="Inputs" width="30%" class="sapUiTinyMarginBegin"/>
        <!-- <Button text="Submit" type="Emphasized" class = "sapUiTinyMarginBegin"></Button> -->
      </content>
      <footer>
       <Bar>
          <contentRight>
             <Button id="save" text="Save" type="Accept" press="Saved"> </Button>
             <Button text="Reset" type="Reject" press="Clear"></Button>
          </contentRight>
          <contentLeft>
              <!-- <Text id="message" text="Details saved successfully" textAlign="Left"></Text> -->
          </contentLeft>
       </Bar>
      </footer>
    </Page>
</mvc:View>